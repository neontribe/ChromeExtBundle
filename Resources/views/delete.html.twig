{% extends '@ChromeExt/base.html.twig' %}

{% block page_title %}Delete Logged time{% endblock %}

{% block main %}

<div class='container'>

    <h4>Are you sure you want to delete this entry?</h4>
    
    <div class='container'></div>
    
    <div>
        <button type="button" class="confirm">Confirm</button>
        <button type="button" class="cancel">Cancel</button>
    </div>
    
</div>
<script>
$('.cancel').on('click', function() {
	window.location = "{{ path( "chrome_ext_list", { "projectUuid": project.uuid, "issueUuid": issue.uuid }) }}"
});

$('.confirm').on('click', function() {
	  window.location = "{{ path( "chrome_ext_delete_impl", { "timesheet": timesheet.id, "extIssue": issue.id }) }}"
});
</script>
{% endblock %}